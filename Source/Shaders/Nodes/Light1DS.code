FIXED4 Light1DS(
	HALF4 lightVec,
	FIXED3 lightHalf,
	HALF4 lightDfColor, 
	HALF3 lightSpColor,
	FIXED3 normal,
	FIXED4 diffuseColor,
	HALF4 specularColor
) {
	return DiffuseSpecularLightPixel(
		lightVec,
		lightHalf,
		lightDfColor,
		lightSpColor, 
		normal, 
		diffuseColor,
		specularColor
	);
}
